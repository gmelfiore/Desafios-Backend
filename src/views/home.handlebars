<div>
    <h1>Productos agregados al carrito</h1>

    <table>
        <thead>
            <th>Id</th>
            <th>Title</th>
            <th>Description</th>
            <th>Price</th>
            <th>Image</th>
            <th>Code</th>
            <th>Stock</th>
            <th>Status</th>
            <th>Category</th>
        </thead>
        <tbody>
            {{#each productos}}
            <tr>
                <td>{{this.id}}</td>
                <td>{{this.title}}</td>
                <td>{{this.description}}</td>
                <td>{{this.price}}</td>
                <td>
                    {{#if this.status}}
                    Activo
                    {{#else}}
                    Inactivo
                    {{/if}}
                </td>
                <td>{{this.code}}</td>
                <td>{{this.stock}}</td>
                <td>{{this.category}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>